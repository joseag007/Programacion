package ejercicio3;

public class Ejercicio3 {

	public static void main(String[] args) {
		
		int num1 = 1, num2 = 1;
		char char1 = 'A', char2 = 'B';
		String cargo = "Estudiante", nombre = "José";
		
		System.out.println("Este es el valor del primer número: " + num1);
		System.out.println("Este es el valor del segundo número" + num2);
		
		System.out.println("Bienvenido, " + cargo + nombre);
		
	}

}
